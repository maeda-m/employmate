<% frame_id = dom_id(question) %>
<%= turbo_stream.append frame_id do %>
  <script>
    (() => {
      const labels = <%== OvertimeField::Component.generate_labels(prev_answer).to_json %>
      const fields = document.querySelectorAll(`#${<%== frame_id.to_json %>} .overtime`)

      labels.forEach((label, i) => {
        const field = fields[i]
        document.querySelector(`label[for="${field.id}"]`).textContent = label
      })
    })()
  </script>
<% end %>
